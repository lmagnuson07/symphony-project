{% extends 'base.html.twig' %}

{% block title %}EdmontonPropertyAssessmentData{% endblock %}

{% block body %}

    <section class="container">

        <h1>EdmontonPropertyAssessmentData</h1>

        <table class="table">
            <tbody>
                {% if epad is not null %}
                    <tr>
                        <th>AccountNumber</th>
                        <td>{{ epad.accountNumber }}</td>
                    </tr>
                    <tr>
                        <th>Suite</th>
                        <td>{{ epad.suite }}</td>
                    </tr>
                    <tr>
                        <th>HouseNumber</th>
                        <td>{{ epad.houseNumber }}</td>
                    </tr>
                    <tr>
                        <th>StreetName</th>
                        <td>{{ epad.streetName }}</td>
                    </tr>
                    <tr>
                        <th>Garage</th>
                        <td>{{ epad.garage ? 'Yes' : 'No' }}</td>
                    </tr>
                    <tr>
                        <th>NeighbourhoodId</th>
                        <td>{{ epad.neighbourhoodId }}</td>
                    </tr>
                    <tr>
                        <th>Neighbourhood</th>
                        <td>{{ epad.neighbourhood }}</td>
                    </tr>
                    <tr>
                        <th>Ward</th>
                        <td>{{ epad.ward }}</td>
                    </tr>
                    <tr>
                        <th>AssessedValue</th>
                        <td>{{ epad.assessedValue }}</td>
                    </tr>
                    <tr>
                        <th>Latitude</th>
                        <td>{{ epad.latitude }}</td>
                    </tr>
                    <tr>
                        <th>Longitude</th>
                        <td>{{ epad.longitude }}</td>
                    </tr>
                    <tr>
                        <th>AssessmentClass1</th>
                        <td>{{ epad.assessmentClass1 }}</td>
                    </tr>
                {% else %}
                    <tr>
                        <td>No records found</td>
                    </tr>
                {% endif %}
            </tbody>
        </table>

        <a href="{{ path('app_admin_epad.list') }}">back to list</a>

        <a href="{{ path('app_admin_epad.edit', {'id': epad.accountNumber}) }}">edit</a>

        {{ include('epad_restapi_crud/_delete_form.html.twig') }}
    </section>

{% endblock %}
